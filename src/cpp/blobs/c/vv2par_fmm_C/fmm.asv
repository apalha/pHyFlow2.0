#else
            const dcmplx d = zr[ix[i]]-zr[ix[j]]+I*(zi[ix[i]]-zi[ix[j]]);
            const double ad = cabs(d);
            if (ad != 0.0) {
              dcmplx P;
              if (ad >= cutoff)
                P = 1.0/d;
              else
                P = -expm1(-shape*ad*ad)/d*scale;
              pr[ix[i]] -= mr[ix[j]]*creal(P);
              pi[ix[i]] -= mr[ix[j]]*cimag(P);
              pr[ix[j]] += mr[ix[i]]*creal(P);
              pi[ix[j]] += mr[ix[i]]*cimag(P);
            }
#endif
          }
#endif /*SSE2DIRECT*/
        }
        break;
      }
    }
    else { // zinv2()
      switch (smooth) {
      case DIRAC:
        for (int i = begin1; i < end1; i++) {
#ifdef SSE2DIRECT
          for (int j = BEGIN(begin2,i); j < end2; j++) {
            const double re = zr[i]-zr[j],im = zi[i]-zi[j];
            const double P = 1.0/(re*re+im*im);
            pr[i] -= (mr[j]*re+mi[j]*im)*P;
            pi[i] += (mr[j]*im-mi[j]*re)*P;
            pr[j] += (mr[i]*re+mi[i]*im)*P;
            pi[j] -= (mr[i]*im-mi[i]*re)*P;
          }
#else
          for (int j = BEGIN(begin2,i); j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = zr[ix[i]]-zr[ix[j]],im = zi[ix[i]]-zi[ix[j]];
            const double P = 1.0/(re*re+im*im);
            pr[ix[i]] -= (mr[ix[j]]*re+mi[ix[j]]*im)*P;
            pi[ix[i]] += (mr[ix[j]]*im-mi[ix[j]]*re)*P;
            pr[ix[j]] += (mr[ix[i]]*re+mi[ix[i]]*im)*P;
            pi[ix[j]] -= (mr[ix[i]]*im-mi[ix[i]]*re)*P;
#else
            const dcmplx P = 1.0/(zr[ix[i]]-zr[ix[j]]+I*(zi[ix[i]]-zi[ix[j]]));
            pr[ix[i]] -= mr[ix[j]]*creal(P)-mi[ix[j]]*cimag(P);
            pi[ix[i]] -= mr[ix[j]]*cimag(P)+mi[ix[j]]*creal(P);
            pr[ix[j]] += mr[ix[i]]*creal(P)-mi[ix[i]]*cimag(P);
            pi[ix[j]] += mr[ix[i]]*cimag(P)+mi[ix[i]]*creal(P);
#endif
          }
#endif /*SSE2DIRECT*/
        }
        break;

      case RANKINE:
        for (int i = begin1; i < end1; i++) {
#ifdef SSE2DIRECT
          for (int j = BEGIN(begin2,i); j < end2; j++) {
            const double re = zr[i]-zr[j],im = zi[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = shape;
            pr[i] -= (mr[j]*re+mi[j]*im)*P;
            pi[i] += (mr[j]*im-mi[j]*re)*P;
            pr[j] += (mr[i]*re+mi[i]*im)*P;
            pi[j] -= (mr[i]*im-mi[i]*re)*P;
          }
#else
          for (int j = BEGIN(begin2,i); j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = zr[ix[i]]-zr[ix[j]],im = zi[ix[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = shape;
            pr[ix[i]] -= (mr[ix[j]]*re+mi[ix[j]]*im)*P;
            pi[ix[i]] += (mr[ix[j]]*im-mi[ix[j]]*re)*P;
            pr[ix[j]] += (mr[ix[i]]*re+mi[ix[i]]*im)*P;
            pi[ix[j]] -= (mr[ix[i]]*im-mi[ix[i]]*re)*P;
#else
            const dcmplx d = zr[ix[i]]-zr[ix[j]]+I*(zi[ix[i]]-zi[ix[j]]);
            const double ad = cabs(d);
            dcmplx P;
            if (ad >= cutoff)
              P = 1.0/d;
            else
              P = conj(d)*shape;
            pr[ix[i]] -= mr[ix[j]]*creal(P)-mi[ix[j]]*cimag(P);
            pi[ix[i]] -= mr[ix[j]]*cimag(P)+mi[ix[j]]*creal(P);
            pr[ix[j]] += mr[ix[i]]*creal(P)-mi[ix[i]]*cimag(P);
            pi[ix[j]] += mr[ix[i]]*cimag(P)+mi[ix[i]]*creal(P);
#endif
          }
#endif
        }
        break;

      case SCULLY:
        for (int i = begin1; i < end1; i++) {
#ifdef SSE2DIRECT
          for (int j = BEGIN(begin2,i); j < end2; j++) {
            const double re = zr[i]-zr[j],im = zi[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = 1.0/(shape+ad)*scale;
            pr[i] -= (mr[j]*re+mi[j]*im)*P;
            pi[i] += (mr[j]*im-mi[j]*re)*P;
            pr[j] += (mr[i]*re+mi[i]*im)*P;
            pi[j] -= (mr[i]*im-mi[i]*re)*P;
          }
#else
          for (int j = BEGIN(begin2,i); j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = zr[ix[i]]-zr[ix[j]],im = zi[ix[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = 1.0/(shape+ad)*scale;
            pr[ix[i]] -= (mr[ix[j]]*re+mi[ix[j]]*im)*P;
            pi[ix[i]] += (mr[ix[j]]*im-mi[ix[j]]*re)*P;
            pr[ix[j]] += (mr[ix[i]]*re+mi[ix[i]]*im)*P;
            pi[ix[j]] -= (mr[ix[i]]*im-mi[ix[i]]*re)*P;
#else
            const dcmplx d = zr[ix[i]]-zr[ix[j]]+I*(zi[ix[i]]-zi[ix[j]]);
            const double ad = cabs(d);
            dcmplx P;
            if (ad >= cutoff)
              P = 1.0/d;
            else
              P = conj(d)/(shape+ad*ad)*scale;
            pr[ix[i]] -= mr[ix[j]]*creal(P)-mi[ix[j]]*cimag(P);
            pi[ix[i]] -= mr[ix[j]]*cimag(P)+mi[ix[j]]*creal(P);
            pr[ix[j]] += mr[ix[i]]*creal(P)-mi[ix[i]]*cimag(P);
            pi[ix[j]] += mr[ix[i]]*cimag(P)+mi[ix[i]]*creal(P);
#endif
          }
#endif /*SSE2DIRECT*/
        }
        break;

      case OSEEN:
        for (int i = begin1; i < end1; i++) {
#ifdef SSE2DIRECT
          for (int j = BEGIN(begin2,i); j < end2; j++) {
            const double re = zr[i]-zr[j],im = zi[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad != 0.0) {
              if (ad >= cutoff)
                P = 1.0/ad;
              else
                P = -expm1(-shape*ad)/ad*scale;
              pr[i] -= (mr[j]*re+mi[j]*im)*P;
              pi[i] += (mr[j]*im-mi[j]*re)*P;
              pr[j] += (mr[i]*re+mi[i]*im)*P;
              pi[j] -= (mr[i]*im-mi[i]*re)*P;
            }
          }
#else
          for (int j = BEGIN(begin2,i); j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = zr[ix[i]]-zr[ix[j]],im = zi[ix[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad != 0.0) {
              if (ad >= cutoff)
                P = 1.0/ad;
              else
                P = -expm1(-shape*ad)/ad*scale;
              pr[ix[i]] -= (mr[ix[j]]*re+mi[ix[j]]*im)*P;
              pi[ix[i]] += (mr[ix[j]]*im-mi[ix[j]]*re)*P;
              pr[ix[j]] += (mr[ix[i]]*re+mi[ix[i]]*im)*P;
              pi[ix[j]] -= (mr[ix[i]]*im-mi[ix[i]]*re)*P;
            }
#else
            const dcmplx d = zr[ix[i]]-zr[ix[j]]+I*(zi[ix[i]]-zi[ix[j]]);
            const double ad = cabs(d);
            if (ad != 0.0) {
              dcmplx P;
              if (ad >= cutoff)
                P = 1.0/d;
              else
                P = -expm1(-shape*ad*ad)/d*scale;
              pr[ix[i]] -= mr[ix[j]]*creal(P)-mi[ix[j]]*cimag(P);
              pi[ix[i]] -= mr[ix[j]]*cimag(P)+mi[ix[j]]*creal(P);
              pr[ix[j]] += mr[ix[i]]*creal(P)-mi[ix[i]]*cimag(P);
              pi[ix[j]] += mr[ix[i]]*cimag(P)+mi[ix[i]]*creal(P);
            }
#endif
          }
#endif /*SSE2DIRECT*/
        }
        break;
      }
    }
  }
}
/*------------------------------------------------------------------------*/
void mpexp_directInteract(double *restrict qr,double *restrict qi,
                          const double *restrict er,const double *restrict ei,
                          const int *restrict jx,int begin1,int end1,
                          const double *restrict zr,const double *restrict zi,
                          const double *restrict mr,const double *restrict mi,
                          const int *restrict ix,int begin2,int end2,
                          int pot,SMOOTHER smooth,
                          double cutoff,double shape,double scale)
/* Direct evaluation of field from potentials inside one box computed
   at points (er,ei) inside another box. */
{
  if (pot == 0) { // logarithmic potential
    if (mi == NULL) { // rlog()
      switch (smooth) {
      case DIRAC:
        for (int i = begin1; i < end1; i++)
          for (int j = begin2; j < end2; j++)
            if (er[jx[i]] != zr[ix[j]] || ei[jx[i]] != zi[ix[j]]) {
#ifdef INLINECOMPLEX_DIRECT
              const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
              const double P = 0.5*log(re*re+im*im);
#else
              const double P = log(hypot(er[jx[i]]-zr[ix[j]],ei[jx[i]]-zi[ix[j]]));
#endif
              qr[jx[i]] -= mr[ix[j]]*P;
            }
        break;

      case RANKINE:
        for (int i = begin1; i < end1; i++)
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 0.5*log(ad);
            else
              P = 0.5*(ad/cutoff)+shape;
#else
            const double ad = hypot(er[jx[i]]-zr[ix[j]],ei[jx[i]]-zi[ix[j]]);
            double P;
            if (ad >= cutoff)
              P = log(ad);
            else
              P = 0.5*(ad/cutoff)*(ad/cutoff)+shape;
#endif
            qr[jx[i]] -= mr[ix[j]]*P;
          }
        break;

      case SCULLY:
        for (int i = begin1; i < end1; i++)
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 0.5*log(ad);
            else
              P = 0.5*log(shape+ad)*scale;
#else
            const double ad = hypot(er[jx[i]]-zr[ix[j]],ei[jx[i]]-zi[ix[j]]);
            double P;
            if (ad >= cutoff)
              P = log(ad);
            else
              P = 0.5*log(shape+ad*ad)*scale;
#endif
            qr[jx[i]] -= mr[ix[j]]*P;
          }
        break;

      case OSEEN:
        for (int i = begin1; i < end1; i++)
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 0.5*log(ad);
            else if (ad != 0.0)
              P = 0.5*(log(ad)+expint(shape*ad))*scale;
            else
              P = -0.5*(log(shape)+gamma_)*scale;
#else
            const double ad = hypot(er[jx[i]]-zr[ix[j]],ei[jx[i]]-zi[ix[j]]);
            double P;
            if (ad >= cutoff)
              P = log(ad);
            else if (ad != 0.0)
              P = (log(ad)+0.5*expint(shape*ad*ad))*scale;
            else
              P = -0.5*(log(shape)+gamma_)*scale;
#endif
            qr[jx[i]] -= mr[ix[j]]*P;
          }
        break;
      }
    }
    else { // zlog()
      switch (smooth) {
      case DIRAC:
        for (int i = begin1; i < end1; i++)
          for (int j = begin2; j < end2; j++)
            if (er[jx[i]] != zr[ix[j]] || ei[jx[i]] != zi[ix[j]]) {
#ifdef INLINECOMPLEX_DIRECT
              const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
              const double P = 0.5*log(re*re+im*im);
#else
              const double P = log(hypot(er[jx[i]]-zr[ix[j]],ei[jx[i]]-zi[ix[j]]));
#endif
              qr[jx[i]] -= mr[ix[j]]*P;
              qi[jx[i]] -= mi[ix[j]]*P;
            }
        break;

      case RANKINE:
        for (int i = begin1; i < end1; i++)
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 0.5*log(ad);
            else
              P = 0.5*(ad/cutoff)+shape;
#else
            const double ad = hypot(er[jx[i]]-zr[ix[j]],ei[jx[i]]-zi[ix[j]]);
            double P;
            if (ad >= cutoff)
              P = log(ad);
            else
              P = 0.5*(ad/cutoff)*(ad/cutoff)+shape;
#endif
            qr[jx[i]] -= mr[ix[j]]*P;
            qi[jx[i]] -= mi[ix[j]]*P;
          }
        break;

      case SCULLY:
        for (int i = begin1; i < end1; i++)
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 0.5*log(ad);
            else
              P = 0.5*log(shape+ad)*scale;
#else
            const double ad = hypot(er[jx[i]]-zr[ix[j]],ei[jx[i]]-zi[ix[j]]);
            double P;
            if (ad >= cutoff)
              P = log(ad);
            else
              P = 0.5*log(shape+ad*ad)*scale;
#endif
            qr[jx[i]] -= mr[ix[j]]*P;
            qi[jx[i]] -= mi[ix[j]]*P;
          }
        break;

      case OSEEN:
        for (int i = begin1; i < end1; i++)
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 0.5*log(ad);
            else if (ad != 0.0)
              P = 0.5*(log(ad)+expint(shape*ad))*scale;
            else
              P = -0.5*(log(shape)+gamma_)*scale;
#else
            const double ad = hypot(er[jx[i]]-zr[ix[j]],ei[jx[i]]-zi[ix[j]]);
            double P;
            if (ad >= cutoff)
              P = log(ad);
            else if (ad != 0.0)
              P = (log(ad)+0.5*expint(shape*ad*ad))*scale;
            else
              P = -0.5*(log(shape)+gamma_)*scale;
#endif
            qr[jx[i]] -= mr[ix[j]]*P;
            qi[jx[i]] -= mi[ix[j]]*P;
          }
        break;
      }
    }
  }
  else { // harmonic potential
    if (mi == NULL) { // rinv()
      switch (smooth) {
      case DIRAC:
        for (int i = begin1; i < end1; i++) {
#ifdef SSE2DIRECT
          int j = begin2;
//          __m128d mzr=_mm_set1_pd(er[jx[i]]);
//          __m128d mzi=_mm_set1_pd(ei[jx[i]]);
          if (j < end2 && j&1) {
            if (er[i] != zr[j] || ei[i] != zi[j]) {
              const double re = er[i]-zr[j],im = ei[i]-zi[j];
              const double P = 1.0/(re*re+im*im);
              qr[i] -= mr[j]*P*re;
              qi[i] += mr[j]*P*im;
            }
            j++;
          }
          __m128d mzr=_mm_set1_pd(er[i]);
          __m128d mzi=_mm_set1_pd(ei[i]);
          __m128d pri=_mm_set1_pd(0);
          __m128d pii=_mm_set1_pd(0);
          for (; j < end2-1; j+=2) {
//            __m128d mre=_mm_set_pd(zr[ix[j]],zr[ix[j+1]]);
//            __m128d mim=_mm_set_pd(zi[ix[j]],zi[ix[j+1]]);
            __m128d mre=_mm_load_pd(&zr[j]);
            __m128d mim=_mm_load_pd(&zi[j]);
//            __m128d* prj=(__m128d*)&pr[j];
//            __m128d* pij=(__m128d*)&pi[j];
            mre=_mm_sub_pd(mre, mzr);
            mim=_mm_sub_pd(mim, mzi);
            __m128d P=_mm_load_pd(&mr[j]);
            __m128d mmr=_mm_add_pd(_mm_mul_pd(mre,mre),_mm_mul_pd(mim,mim));
            P=_mm_div_pd(P,mmr);
            double ad;
            _mm_storel_pd(&ad, mmr);
            if(ad==0) {
              double Pd=0;
              P=_mm_loadl_pd(P, &Pd);
            }
            _mm_storeh_pd(&ad, mmr);
            if(ad==0) {
              double Pd=0;
              P=_mm_loadh_pd(P, &Pd);
            }
//            mmr=_mm_set_pd(mr[ix[j]], mr[ix[j+1]]);
//            mmr=_mm_load_pd(&mr[j]);
//            mmr=_mm_mul_pd(mmr, P);
            pri=_mm_add_pd(pri, _mm_mul_pd(P, mre));
            pii=_mm_sub_pd(pii, _mm_mul_pd(P, mim));
          }
          double VC16ALIGN a2[2] GCC16ALIGN;
          _mm_store_pd(a2, pri);
          qr[i]+=a2[0]+a2[1];
          _mm_store_pd(a2, pii);
          qi[i]+=a2[0]+a2[1];
          if (j < end2) {
            if (er[i] != zr[j] || ei[i] != zi[j]) {
              const double re = er[i]-zr[j],im = ei[i]-zi[j];
              const double P = 1.0/(re*re+im*im);
              qr[i] -= mr[j]*P*re;
              qi[i] += mr[j]*P*im;
            }
          }
#else /*SSE2DIRECT*/
          for (int j = begin2; j < end2; j++)
            if (er[jx[i]] != zr[ix[j]] || ei[jx[i]] != zi[ix[j]]) {
#ifdef INLINECOMPLEX_DIRECT
              const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
              const double P = 1.0/(re*re+im*im);
              qr[jx[i]] -= mr[ix[j]]*P*re;
              qi[jx[i]] += mr[ix[j]]*P*im;
#else
              const dcmplx P = 1.0/(er[jx[i]]-zr[ix[j]]+I*(ei[jx[i]]-zi[ix[j]]));
              qr[jx[i]] -= mr[ix[j]]*creal(P);
              qi[jx[i]] -= mr[ix[j]]*cimag(P);
#endif
            }
#endif /*SSE2DIRECT*/
        }
        break;

      case RANKINE:
        for (int i = begin1; i < end1; i++) {
          #ifdef SSE2DIRECT
          int j = begin2;
          if (j < end2&&j&1) {
            const double re = er[i]-zr[j],im = ei[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = shape;
            qr[i] -= mr[j]*P*re;
            qi[i] += mr[j]*P*im;
            j++;
          }
          __m128d mzr=_mm_set1_pd(er[i]);
          __m128d mzi=_mm_set1_pd(ei[i]);
          __m128d pri=_mm_set1_pd(0);
          __m128d pii=_mm_set1_pd(0);
          for (; j < end2-1; j+=2) {
            __m128d mre=_mm_load_pd(&zr[j]);
            __m128d mim=_mm_load_pd(&zi[j]);
            mre=_mm_sub_pd(mre, mzr);
            mim=_mm_sub_pd(mim, mzi);
            __m128d mmr=_mm_add_pd(_mm_mul_pd(mre,mre),_mm_mul_pd(mim,mim));
            __m128d P=_mm_div_pd(_mm_set1_pd(1.0),mmr);
            double ad;
            _mm_storel_pd(&ad, mmr);
            if(ad<cutoff) {
              P=_mm_loadl_pd(P, &shape);
            }
            _mm_storeh_pd(&ad, mmr);
            if(ad<cutoff) {
              P=_mm_loadh_pd(P, &shape);
            }
            mmr=_mm_load_pd(&mr[j]);
            mmr=_mm_mul_pd(mmr,P);
            pri=_mm_add_pd(pri, _mm_mul_pd(mmr, mre));
            pii=_mm_sub_pd(pii, _mm_mul_pd(mmr, mim));
          }
          double VC16ALIGN a2[2] GCC16ALIGN;
          _mm_store_pd(a2, pri);
          qr[i]+=a2[0]+a2[1];
          _mm_store_pd(a2, pii);
          qi[i]+=a2[0]+a2[1];
          if (j < end2) {
            const double re = er[i]-zr[j],im = ei[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = shape;
            qr[i] -= mr[j]*P*re;
            qi[i] += mr[j]*P*im;
          }
#else /*SSE2DIRECT*/
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = shape;
            qr[jx[i]] -= mr[ix[j]]*P*re;
            qi[jx[i]] += mr[ix[j]]*P*im;
#else
            const dcmplx d = er[jx[i]]-zr[ix[j]]+I*(ei[jx[i]]-zi[ix[j]]);
            const double ad = cabs(d);
            dcmplx P;
            if (ad >= cutoff)
              P = 1.0/d;
            else
              P = conj(d)*shape;
            qr[jx[i]] -= mr[ix[j]]*creal(P);
            qi[jx[i]] -= mr[ix[j]]*cimag(P);
#endif
          }
#endif
        }
        break;

      case SCULLY:
        for (int i = begin1; i < end1; i++) {
#ifdef SSE2DIRECT
          int j = begin2;
          if (j < end2&&j&1) {
            const double re = er[i]-zr[j],im = ei[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = 1.0/(shape+ad)*scale;
            qr[i] -= mr[j]*P*re;
            qi[i] += mr[j]*P*im;
            j++;
          }
          __m128d mzr=_mm_set1_pd(er[i]);
          __m128d mzi=_mm_set1_pd(ei[i]);
          __m128d pri=_mm_set1_pd(0);
          __m128d pii=_mm_set1_pd(0);
          for (; j < end2-1; j+=2) {
            __m128d mre=_mm_load_pd(&zr[j]);
            __m128d mim=_mm_load_pd(&zi[j]);
            mre=_mm_sub_pd(mre, mzr);
            mim=_mm_sub_pd(mim, mzi);
            __m128d mmr=_mm_add_pd(_mm_mul_pd(mre,mre),_mm_mul_pd(mim,mim));
            __m128d P=_mm_div_pd(_mm_set1_pd(1.0),mmr);
            double ad;
            _mm_storel_pd(&ad, mmr);
            if(ad<cutoff) {
              double Pd = 1.0/(shape+ad)*scale;
              P=_mm_loadl_pd(P, &Pd);
            }
            _mm_storeh_pd(&ad, mmr);
            if(ad<cutoff) {
              double Pd = 1.0/(shape+ad)*scale;
              P=_mm_loadh_pd(P, &Pd);
            }
            mmr=_mm_load_pd(&mr[j]);
            mmr=_mm_mul_pd(mmr,P);
            pri=_mm_add_pd(pri, _mm_mul_pd(mmr, mre));
            pii=_mm_sub_pd(pii, _mm_mul_pd(mmr, mim));
          }
          double VC16ALIGN a2[2] GCC16ALIGN;
          _mm_store_pd(a2, pri);
          qr[i]+=a2[0]+a2[1];
          _mm_store_pd(a2, pii);
          qi[i]+=a2[0]+a2[1];
          if (j < end2) {
            const double re = er[i]-zr[j],im = ei[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = 1.0/(shape+ad)*scale;
            qr[i] -= mr[j]*P*re;
            qi[i] += mr[j]*P*im;
          }
#else /*SSE2DIRECT*/
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = 1.0/(shape+ad)*scale;
            qr[jx[i]] -= mr[ix[j]]*P*re;
            qi[jx[i]] += mr[ix[j]]*P*im;
#else
            const dcmplx d = er[jx[i]]-zr[ix[j]]+I*(ei[jx[i]]-zi[ix[j]]);
            const double ad = cabs(d);
            dcmplx P;
            if (ad >= cutoff)
              P = 1.0/d;
            else
              P = conj(d)/(shape+ad*ad)*scale;
            qr[jx[i]] -= mr[ix[j]]*creal(P);
            qi[jx[i]] -= mr[ix[j]]*cimag(P);
#endif
          }
#endif /*SSE2DIRECT*/
        }
        break;

      case OSEEN:
        for (int i = begin1; i < end1; i++) {
#ifdef SSE2DIRECT
          int j = begin2;
          if (j < end2&&j&1) {
            const double re = er[i]-zr[j],im = ei[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad != 0.0) {
              if (ad >= cutoff)
                P = 1.0/ad;
              else
                P = -expm1(-shape*ad)/ad*scale;
              qr[i] -= mr[j]*P*re;
              qi[i] += mr[j]*P*im;
            }
            j++;
          }
//          __m128d mzr=_mm_set1_pd(er[jx[i]]);
//          __m128d mzi=_mm_set1_pd(ei[jx[i]]);
          __m128d mzr=_mm_set1_pd(er[i]);
          __m128d mzi=_mm_set1_pd(ei[i]);
          __m128d pri=_mm_set1_pd(0);
          __m128d pii=_mm_set1_pd(0);
          for (; j < end2-1; j+=2) {
//            __m128d mre=_mm_set_pd(zr[ix[j]],zr[ix[j+1]]);
//            __m128d mim=_mm_set_pd(zi[ix[j]],zi[ix[j+1]]);
            __m128d mre=_mm_load_pd(&zr[j]);
            __m128d mim=_mm_load_pd(&zi[j]);
//            __m128d* prj=(__m128d*)&pr[j];
//            __m128d* pij=(__m128d*)&pi[j];
            mre=_mm_sub_pd(mre, mzr);
            mim=_mm_sub_pd(mim, mzi);
            __m128d P=_mm_load_pd(&mr[j]);
            __m128d mad=_mm_add_pd(_mm_mul_pd(mre,mre),_mm_mul_pd(mim,mim));
            P=_mm_div_pd(P,mad);
            double ad;
            _mm_storel_pd(&ad, mad);
            if(ad<cutoff) {
              if(ad!=0.0) {
                double Pd;
                _mm_storel_pd(&Pd, P);
                Pd*= -expm1(-shape*ad)*scale;
                P=_mm_loadl_pd(P, &Pd);
              }
              else {
                double Pd=0;
                P=_mm_loadl_pd(P, &Pd);
              }
            }
            _mm_storeh_pd(&ad, mad);
            if(ad<cutoff) {
              if(ad!=0.0) {
                double Pd;
                _mm_storeh_pd(&Pd, P);
                Pd*= -expm1(-shape*ad)*scale;
                P=_mm_loadh_pd(P, &Pd);
              }
              else {
                double Pd=0;
                P=_mm_loadh_pd(P, &Pd);
              }
            }
//            mmr=_mm_set_pd(mr[ix[j]], mr[ix[j+1]]);
//            mmr=_mm_mul_pd(mmr, P);
            pri=_mm_add_pd(pri, _mm_mul_pd(P, mre));
            pii=_mm_sub_pd(pii, _mm_mul_pd(P, mim));
          }
          double VC16ALIGN a2[2] GCC16ALIGN;
          _mm_store_pd(a2, pri);
          qr[i]+=a2[0]+a2[1];
          _mm_store_pd(a2, pii);
          qi[i]+=a2[0]+a2[1];
          if (j < end2) {
            const double re = er[i]-zr[j],im = ei[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad != 0.0) {
              if (ad >= cutoff)
                P = 1.0/ad;
              else
                P = -expm1(-shape*ad)/ad*scale;
              qr[i] -= mr[j]*P*re;
              qi[i] += mr[j]*P*im;
            }
          }
#else /*SSE2DIRECT*/
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad != 0.0) {
              if (ad >= cutoff)
                P = 1.0/ad;
              else
                P = -expm1(-shape*ad)/ad*scale;
              qr[jx[i]] -= mr[ix[j]]*P*re;
              qi[jx[i]] += mr[ix[j]]*P*im;
            }
#else
            const dcmplx d = er[jx[i]]-zr[ix[j]]+I*(ei[jx[i]]-zi[ix[j]]);
            const double ad = cabs(d);
            if (ad != 0.0) {
              dcmplx P;
              if (ad >= cutoff)
                P = 1.0/d;
              else
                P = -expm1(-shape*ad*ad)/d*scale;
              qr[jx[i]] -= mr[ix[j]]*creal(P);
              qi[jx[i]] -= mr[ix[j]]*cimag(P);
            }
#endif
          }
#endif /*SSE2DIRECT*/
        }
        break;
      }
    }
    else { // zinv()
      switch (smooth) {
      case DIRAC:
        for (int i = begin1; i < end1; i++) {
#ifdef SSE2DIRECT
          for (int j = begin2; j < end2; j++)
            if (er[i] != zr[j] || ei[i] != zi[j]) {
              const double re = er[i]-zr[j],im = ei[i]-zi[j];
              const double P = 1.0/(re*re+im*im);
              qr[i] -= (mr[j]*re+mi[j]*im)*P;
              qi[i] += (mr[j]*im-mi[j]*re)*P;
            }
#else
          for (int j = begin2; j < end2; j++)
            if (er[jx[i]] != zr[ix[j]] || ei[jx[i]] != zi[ix[j]]) {
#ifdef INLINECOMPLEX_DIRECT
              const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
              const double P = 1.0/(re*re+im*im);
              qr[jx[i]] -= (mr[ix[j]]*re+mi[ix[j]]*im)*P;
              qi[jx[i]] += (mr[ix[j]]*im-mi[ix[j]]*re)*P;
#else
              const dcmplx P = 1.0/(er[jx[i]]-zr[ix[j]]+I*(ei[jx[i]]-zi[ix[j]]));
              qr[jx[i]] -= mr[ix[j]]*creal(P)-mi[ix[j]]*cimag(P);
              qi[jx[i]] -= mr[ix[j]]*cimag(P)+mi[ix[j]]*creal(P);
#endif
            }
#endif /*SSE2DIRECT*/
        }
        break;

      case RANKINE:
        for (int i = begin1; i < end1; i++) {
#ifdef SSE2DIRECT
          for (int j = begin2; j < end2; j++) {
            const double re = er[i]-zr[j],im = ei[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = shape;
            qr[i] -= (mr[j]*re+mi[j]*im)*P;
            qi[i] += (mr[j]*im-mi[j]*re)*P;
          }
#else
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = shape;
            qr[jx[i]] -= (mr[ix[j]]*re+mi[ix[j]]*im)*P;
            qi[jx[i]] += (mr[ix[j]]*im-mi[ix[j]]*re)*P;
#else
            const dcmplx d = er[jx[i]]-zr[ix[j]]+I*(ei[jx[i]]-zi[ix[j]]);
            const double ad = cabs(d);
            dcmplx P;
            if (ad >= cutoff)
              P = 1.0/d;
            else
              P = conj(d)*shape;
            qr[jx[i]] -= mr[ix[j]]*creal(P)-mi[ix[j]]*cimag(P);
            qi[jx[i]] -= mr[ix[j]]*cimag(P)+mi[ix[j]]*creal(P);
#endif
          }
#endif /*SSE2DIRECT*/
        }
        break;

      case SCULLY:
        for (int i = begin1; i < end1; i++) {
#ifdef SSE2DIRECT
          for (int j = begin2; j < end2; j++) {
            const double re = er[i]-zr[j],im = ei[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = 1.0/(shape+ad)*scale;
            qr[i] -= (mr[j]*re+mi[j]*im)*P;
            qi[i] += (mr[j]*im-mi[j]*re)*P;
          }
#else
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad >= cutoff)
              P = 1.0/ad;
            else
              P = 1.0/(shape+ad)*scale;
            qr[jx[i]] -= (mr[ix[j]]*re+mi[ix[j]]*im)*P;
            qi[jx[i]] += (mr[ix[j]]*im-mi[ix[j]]*re)*P;
#else
            const dcmplx d = er[jx[i]]-zr[ix[j]]+I*(ei[jx[i]]-zi[ix[j]]);
            const double ad = cabs(d);
            dcmplx P;
            if (ad >= cutoff)
              P = 1.0/d;
            else
              P = conj(d)/(shape+ad*ad)*scale;
            qr[jx[i]] -= mr[ix[j]]*creal(P)-mi[ix[j]]*cimag(P);
            qi[jx[i]] -= mr[ix[j]]*cimag(P)+mi[ix[j]]*creal(P);
#endif
          }
#endif /*SSE2DIRECT*/
        }
        break;

      case OSEEN:
        for (int i = begin1; i < end1; i++) {
#ifdef SSE2DIRECT
          for (int j = begin2; j < end2; j++) {
            const double re = er[i]-zr[j],im = ei[i]-zi[j];
            const double ad = re*re+im*im;
            double P;
            if (ad != 0.0) {
              if (ad >= cutoff)
                P = 1.0/ad;
              else
                P = -expm1(-shape*ad)/ad*scale;
              qr[i] -= (mr[j]*re+mi[j]*im)*P;
              qi[i] += (mr[j]*im-mi[j]*re)*P;
            }
          }
#else
          for (int j = begin2; j < end2; j++) {
#ifdef INLINECOMPLEX_DIRECT
            const double re = er[jx[i]]-zr[ix[j]],im = ei[jx[i]]-zi[ix[j]];
            const double ad = re*re+im*im;
            double P;
            if (ad != 0.0) {
              if (ad >= cutoff)
                P = 1.0/ad;
              else
                P = -expm1(-shape*ad)/ad*scale;
              qr[jx[i]] -= (mr[ix[j]]*re+mi[ix[j]]*im)*P;
              qi[jx[i]] += (mr[ix[j]]*im-mi[ix[j]]*re)*P;
            }
#else
            const dcmplx d = er[jx[i]]-zr[ix[j]]+I*(ei[jx[i]]-zi[ix[j]]);
            const double ad = cabs(d);
            if (ad != 0.0) {
              dcmplx P;
              if (ad >= cutoff)
                P = 1.0/d;
              else
                P = -expm1(-shape*ad*ad)/d*scale;
              qr[jx[i]] -= mr[ix[j]]*creal(P)-mi[ix[j]]*cimag(P);
              qi[jx[i]] -= mr[ix[j]]*cimag(P)+mi[ix[j]]*creal(P);
            }
